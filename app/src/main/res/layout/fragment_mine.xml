<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/mine_info"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y400"
        android:fitsSystemWindows="true"
        android:orientation="vertical"
        android:background="@drawable/mine_background">
        <ImageView
            android:id="@+id/mine_message"
            android:layout_width="@dimen/x40"
            android:layout_height="@dimen/x100"
            android:layout_alignParentEnd="true"
            android:paddingTop="@dimen/x60"
            android:layout_marginRight="@dimen/x104"
            android:src="@drawable/message"/>
        <ImageView
            android:id="@+id/mine_setting"
            android:layout_width="@dimen/x67"
            android:layout_height="@dimen/x100"
            android:layout_alignParentEnd="true"
            android:paddingTop="@dimen/x60"
            android:paddingRight="@dimen/x27"
            android:src="@drawable/setting"/>

        <ImageView
            android:id="@+id/mine_img"
            android:layout_width="@dimen/x104"
            android:layout_height="@dimen/y104"
            android:src="@color/colorPrimary"
            android:layout_marginTop="@dimen/x180"
            android:layout_marginLeft="@dimen/x30"
            />
        <TextView
            android:id="@+id/mine_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/textSize42"
            android:textColor="@color/text_black"
            android:textStyle="bold"
            android:layout_marginTop="@dimen/x180"
            android:layout_marginLeft="@dimen/y150"
            android:text=""/>
        <TextView
            android:id="@+id/mine_tel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/textSize27"
            android:textColor="@color/text_black"
            android:layout_marginTop="@dimen/x250"
            android:includeFontPadding="false"
            android:layout_marginLeft="@dimen/y150"
            android:text=""/>

        <ImageView
            android:layout_width="@dimen/x19"
            android:layout_height="@dimen/x19"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="@dimen/y223"
            android:layout_marginRight="@dimen/x35"
            android:src="@drawable/jiantou_you_hei"/>
    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/y400"
        android:orientation="vertical">
        <RelativeLayout
            android:id="@+id/mine_score"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y115">
            <ImageView
                android:layout_width="@dimen/x38"
                android:layout_height="@dimen/x38"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x35"
                android:src="@drawable/mine_jifen"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/textSize31"
                android:textColor="@color/text_black"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x100"
                android:text="我的积分"/>

            <TextView
                android:id="@+id/mine_score_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/text_price0c"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="@dimen/x68"
                android:text="15440分"/>
            
            <ImageView
                android:layout_width="@dimen/x19"
                android:layout_height="@dimen/x19"
                android:src="@drawable/jiantou_you"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="@dimen/x35"
                />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/x2"
            android:layout_marginLeft="@dimen/x35"
            android:layout_marginRight="@dimen/x35"
            android:background="@color/line_f2"/>

        <RelativeLayout
            android:id="@+id/mine_apply"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y115">
            <ImageView
                android:layout_width="@dimen/x38"
                android:layout_height="@dimen/x38"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x35"
                android:src="@drawable/mine_shengou"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/textSize31"
                android:textColor="@color/text_black"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x100"
                android:text="我的申购"/>

            <ImageView
                android:layout_width="@dimen/x19"
                android:layout_height="@dimen/x19"
                android:src="@drawable/jiantou_you"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="@dimen/x35"
                />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/x2"
            android:layout_marginLeft="@dimen/x35"
            android:layout_marginRight="@dimen/x35"
            android:background="@color/line_f2"/>
        <RelativeLayout
            android:id="@+id/mine_address"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y115">
            <ImageView
                android:layout_width="@dimen/x38"
                android:layout_height="@dimen/x38"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x35"
                android:src="@drawable/mine_address"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/textSize31"
                android:textColor="@color/text_black"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x100"
                android:text="我的收货地址"/>

            <ImageView
                android:layout_width="@dimen/x19"
                android:layout_height="@dimen/x19"
                android:src="@drawable/jiantou_you"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="@dimen/x35"
                />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/x2"
            android:layout_marginLeft="@dimen/x35"
            android:layout_marginRight="@dimen/x35"
            android:background="@color/line_f2"/>
        <RelativeLayout
            android:id="@+id/mine_share"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y115">
            <ImageView
                android:layout_width="@dimen/x38"
                android:layout_height="@dimen/x38"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x35"
                android:src="@drawable/mine_share"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/textSize31"
                android:textColor="@color/text_black"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x100"
                android:text="推荐有礼"/>

            <ImageView
                android:layout_width="@dimen/x19"
                android:layout_height="@dimen/x19"
                android:src="@drawable/jiantou_you"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="@dimen/x35"
                />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/x2"
            android:layout_marginLeft="@dimen/x35"
            android:layout_marginRight="@dimen/x35"
            android:background="@color/line_f2"/>
        <RelativeLayout
            android:id="@+id/mine_help"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y115">
            <ImageView
                android:layout_width="@dimen/x38"
                android:layout_height="@dimen/x38"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x35"
                android:src="@drawable/mine_help"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/textSize31"
                android:textColor="@color/text_black"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x100"
                android:text="帮助中心"/>

            <ImageView
                android:layout_width="@dimen/x19"
                android:layout_height="@dimen/x19"
                android:src="@drawable/jiantou_you"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="@dimen/x35"
                />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/x2"
            android:layout_marginLeft="@dimen/x35"
            android:layout_marginRight="@dimen/x35"
            android:background="@color/line_f2"/>
        <RelativeLayout
            android:id="@+id/mine_us"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y115">
            <ImageView
                android:layout_width="@dimen/x38"
                android:layout_height="@dimen/x38"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x35"
                android:src="@drawable/mine_constract"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/textSize31"
                android:textColor="@color/text_black"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x100"
                android:text="联系我们"/>

            <ImageView
                android:layout_width="@dimen/x19"
                android:layout_height="@dimen/x19"
                android:src="@drawable/jiantou_you"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="@dimen/x35"
                />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/x2"
            android:layout_marginLeft="@dimen/x35"
            android:layout_marginRight="@dimen/x35"
            android:background="@color/line_f2"/>

    </LinearLayout>



</RelativeLayout>